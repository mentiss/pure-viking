{
  "name": "pure-vikings-app",
  "version": "4.0.0",
  "description": "App VTT multi-JDR",
  "main": "src/server/server.js",
  "scripts": {
    "start": "node src/server/server.js",
    "dev": "concurrently \"npm run server-dev\" \"npm run client-dev\"",
    "server-dev": "nodemon src/server/server.js",
    "client-dev": "cd src/client && npm run dev",
    "build": "cd src/client && npm run build",
    "init-db": "node src/server/init-db.js",
    "migrate": "node src/server/migrate.js"
  },
  "keywords": [
    "vikings",
    "rpg",
    "saga"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^9.2.2",
    "body-parser": "^1.20.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.3",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11"
  }
}
